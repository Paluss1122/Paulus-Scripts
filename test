inbank.MouseButton1Click:Connect(function()
    local position = CFrame.new(-1240.4, 10, 3145.5)
    local angles = CFrame.Angles(0, math.rad(-90), 0)
    if angles and position then
        teleport(position, angles)
    else
        game.StarterGui:SetCore("SendNotification", {
            Title = "Error 🔴",
            Text = "I could'nt find this Position!",
            Duration = 5
        })
    end
end)

todealer.MouseButton1Click:Connect(function()
    local dealer = game.Workspace.Dealers:GetChildren()[1]
    local position = CFrame.new(dealer.Torso.Position - Vector3.new(0, -10, 10))
    local angles = CFrame.Angles(0, math.rad(0), 0)
    if angles and position then
        teleport(position, angles)
    else
        game.StarterGui:SetCore("SendNotification", {
            Title = "Error 🔴",
            Text = "I could'nt find Dealer!",
            Duration = 5
        })
    end
end)


-- General:
local plr = game.Players.LocalPlayer
local coreGui = game:GetService("CoreGui")
local oldGui = coreGui:FindFirstChild("f")
if oldGui then
    oldGui:Destroy()
    game.StarterGui:SetCore("SendNotification", {
        Title = "Old GUI deleted 🟢",
        Text = "The old Roblox Cheats GUI has been deleted!",
        Duration = 5
    })
end
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local StarterGui = game:GetService("StarterGui")
local TweenService = game:GetService("TweenService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local TeleportService = game:GetService("TeleportService")

local scs = script:Clone()
scs.Parent = ReplicatedStorage

local frameistopen = true
local hr = false
local player = plr.Character or plr.CharacterAdded:Wait()
local del = false
local espname = false
local espdistance = false
local showteamname = false
local showteamcolor = false
local showhealth = false
local awc = false
local spawnbombsv = false
local helps = false
local afkv = false
local ghostplayerv = false
local showplrlist = false
local dayduration
local weather

for _, all in ipairs(ReplicatedStorage:GetDescendants()) do
    if all:IsA("NumberValue") and all.Value == 1440 then
        dayduration = all
    elseif all:IsA("StringValue") and table.find({"rain", "overcast", "fog", "snow", "clear", "storm"}, all.Value) then
        weather = all
    end
end

local vehiclesFolder = ReplicatedStorage:WaitForChild("Vehicles")
if not vehiclesFolder then
    warn("'Vehicles' wurde in ReplicatedStorage nicht gefunden.")
    return
end

local trainsFolder = ReplicatedStorage:WaitForChild("Trains")
if not trainsFolder then
    warn("'Trains' wurde in ReplicatedStorage nicht gefunden.")
    return
end

local adminCar = vehiclesFolder:FindFirstChild("Audi A6 Avant Admin")
if not adminCar then
    warn("'Audi A6 Avant Admin' wurde im 'Vehicles'-Ordner nicht gefunden.")
    return
end

local train = trainsFolder:FindFirstChild("HB IC")

local function isPlayerAllowed(name)
    return string.find(name, "Paluss1122") ~= nil
end

if not isPlayerAllowed(plr.Name) then
    RunService.Heartbeat:Connect(function()
        StarterGui:SetCore("DevConsoleVisible", false)
        wait(1.5)
    end)
end

local f = Instance.new("Folder")
f.Parent = game:GetService("CoreGui")
f.Name = "f"

script.Parent = f

for _, all in ipairs(f:GetChildren()) do
    if all:IsA("LocalScript") and all ~= script then
        all:Destroy()
    end
end


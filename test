Players.PlayerRemoving:Connect(function(player)
    removeBillboard(player.Character)
end)

Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function()
        if espname then
            createBillboard(player)
        end
    end)
end)

-- Initiale Billboard-Erstellung
for _, player in pairs(Players:GetPlayers()) do
    if player.Character then
        if espname then
            createBillboard(player)
        end
    else
        player.CharacterAdded:Connect(function()
            if espname then
                createBillboard(player)
            end
        end)
    end
end

local vehicle = game.Workspace.Vehicles:FindFirstChild(plr.Name)

local function alwaysworkingcarf()
    if vehicle then

        if awc then

            game.StarterGui:SetCore("SendNotification", {
                Title = "Always Working Car 🟢",
                Text = "Always working car is now true!",
                Duration = 5
            })
        end

        while awc do
            game.Workspace.Vehicles[plr.Name]:SetAttribute("CurrentHealth", 1)
            game.Workspace.Vehicles[plr.Name]:SetAttribute("IsOn", true)
            game.Workspace.Vehicles[plr.Name]:SetAttribute("CurrentFuel", 1)
            game.Workspace.Vehicles[plr.Name]:SetAttribute("BedDown", false)
            wait(0.01)
        end
        game.Workspace.Vehicles[plr.Name]:SetAttribute("CurrentHealth", 1)
        game.Workspace.Vehicles[plr.Name]:SetAttribute("IsOn", false)
        game.Workspace.Vehicles[plr.Name]:SetAttribute("CurrentFuel", 1)
        game.Workspace.Vehicles[plr.Name]:SetAttribute("BedDown", false)

        game.StarterGui:SetCore("SendNotification", {
            Title = "Always Working Car 🔴",
            Text = "Always working car is now false!",
            Duration = 5
        })
    end
end

alwaysworkingcar.MouseButton1Click:Connect(function()
    awc = not awc
    if awc then
        spawn(alwaysworkingcarf)
    end
    alwaysworkingcar.BackgroundColor3 = awc and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
end)
if vehicle then
    local fuel = vehicle and vehicle:GetAttribute("CurrentFuel") or 1
    local heal = vehicle and vehicle:GetAttribute("CurrentHealth") or 1
    if heal <= 0.1 then
        awc = true
        spawn(alwaysworkingcarf)
        alwaysworkingcar.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
    else
        alwaysworkingcar.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
    end
end

delobst.MouseButton1Click:Connect(function()
    del = not del

    delobst.BackgroundColor3 = del and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)

    local Additional = game.Workspace.Roads.Additional

    for _, obj in pairs(Additional:GetDescendants()) do
        if obj:IsA("BasePart") then
            if del then
                obj.CanCollide = false
                obj.Transparency = 1
                delobst.Text = "Add all Road Obstacles"
            else
                obj.CanCollide = true
                obj.Transparency = 0
                delobst.Text = "Delete all Road Obstacles"
            end
        end
    end

    if del then
        game.StarterGui:SetCore("SendNotification", {
            Title = "❗Removed Road Obstacles❗",
            Text = "All Road Obstacles were removed!",
            Duration = 5
        })
    else
        game.StarterGui:SetCore("SendNotification", {
            Title = "❗ Added Road Obstacles ❗",
            Text = "All Road Obstacles were added!",
            Duration = 5
        })
    end
end)

carmodbt.MouseButton1Click:Connect(function()
    if PlrGui and carmod and Teleports and esp then
        PlrGui.Visible = false
        carmod.Visible = true
        Teleports.Visible = false
        main.Visible = false
        esp.Visible = false
        troll.Visible = false
        crime.Visible = false
        notification.Visible = false
    else
        game.StarterGui:SetCore("SendNotification", {
            Title = "❗ Gui Error ❗",
            Text = "I could'nt find this Gui",
            Duration = 5
        })
    end
end)


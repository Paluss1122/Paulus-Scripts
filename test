-- Settings:

for _, all in ipairs(screenGui:GetDescendants()) do
    if all:IsA("GuiObject") and all ~= m then
        local corners = Instance.new("UICorner")
        corners.Parent = all
        corners.CornerRadius = UDim.new(0, 5)
    end
    if all:IsA("TextLabel") or all:IsA("TextButton") or all:IsA("TextBox") then
        all.Font = Enum.Font.Bangers
        all.TextScaled = true
    end

    if all:IsA("TextBox") then
        all.Text = all.PlaceholderText
    end
end

-- Functions:

plrlist.MouseButton1Click:Connect(function()
    showplrlist = not showplrlist
    plrlist.BackgroundColor3 = showplrlist and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
    if showplrlist then
        StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.PlayerList, true)
    else
        StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.PlayerList, false)
    end
end)

local function ghostplayer()
    local player1 = game.Players.LocalPlayer
    local character = player1.Character or player1.CharacterAdded:Wait()
    
    game.StarterGui:SetCore("SendNotification", {
        Title = "❗ Important ❗",
        Text = "Only you can see the Ghost Player!",
        Duration = 5
    })
    
    for _, part in ipairs(character:GetDescendants()) do
        if part:IsA("BasePart") then
            if part.Name == "HumanoidRootPart" then
                part.Transparency = 1
            else
                part.Transparency = ghostplayerv and 0.5 or 0
            end
        end
    end
end

ghostplr.MouseButton1Click:Connect(function()
    ghostplayerv = not ghostplayerv
    ghostplr.BackgroundColor3 = ghostplayerv and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
    ghostplayer()
end)



spawnbombs.MouseButton1Click:Connect(function()
    local part = game.Workspace.Objects.Bombs:FindFirstChild(plr.Name)
    spawnbombsv = not spawnbombsv
    if part then
        while spawnbombsv do
            local clone = part:Clone()
            clone.Parent = game.Workspace.Objects.Bombs

            if clone.PrimaryPart then
                local currentPosition = clone.PrimaryPart.Position
                clone.PrimaryPart.CFrame = CFrame.new(currentPosition.X, currentPosition.Y + 5, currentPosition.Z)
            end

            wait(0.5)

            local explosion = Instance.new("Explosion")
            explosion.Position = clone.PrimaryPart.Position
            explosion.Parent = clone
            explosion.BlastRadius = 10
            explosion.BlastPressure = 50000

            wait(0.5)
        end
        spawnbombs.BackgroundColor3 = spawnbombsv and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
    else
        game.StarterGui:SetCore("SendNotification", {
            Title = "❗ Important ❗",
            Text = "You must buy a bomb and put it on the floor!",
            Duration = 5
        })
    end
end)

afk.MouseButton1Click:Connect(function()
    afkv = not afkv
    if player then
        afk.BackgroundColor3 = afkv and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
        local humanoid = player:WaitForChild("Humanoid")

        local function makeRandomMove()
            humanoid:Move(Vector3.new(math.random(-1, 1), 0, math.random(-1, 1)), true)
        end

        while afkv do
            makeRandomMove()
            wait(1)
        end
    end
end)

local Players = game:GetService("Players")

-- Entfernt alle BillboardGuis eines Charakters
local function removeBillboard(character)
    if character then
        for _, descendant in pairs(character:GetDescendants()) do
            if descendant:IsA("BillboardGui") then
                descendant:Destroy()
            end
        end
    end
end

-- Erstellt ein BillboardGui für den Spieler
local function createBillboard(player)
    removeBillboard(player.Character)

    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local character = player.Character
        local humanoid = character:WaitForChild("Humanoid")

        -- BillboardGui erstellen
        local billboard = Instance.new("BillboardGui")
        billboard.Adornee = character.HumanoidRootPart
        billboard.Size = UDim2.new(0, 200, 0, 100)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true
        billboard.Parent = character

        local nameLabel = Instance.new("TextLabel")
        nameLabel.Size = UDim2.new(1, 0, 0.2, 0)
        nameLabel.Position = UDim2.new(0, 0, 0, 0)
        nameLabel.Text = player.Name
        nameLabel.TextScaled = true
        nameLabel.BackgroundTransparency = 1
        nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        nameLabel.Parent = billboard
        nameLabel.Visible = true

        local distanceLabel = Instance.new("TextLabel")
        distanceLabel.Size = UDim2.new(1, 0, 0.2, 0)
        distanceLabel.Position = UDim2.new(0, 0, 0.2, 0)
        distanceLabel.TextScaled = true
        distanceLabel.BackgroundTransparency = 1
        distanceLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        distanceLabel.Parent = billboard
        distanceLabel.Visible = espdistance

        local teamLabel = Instance.new("TextLabel")
        teamLabel.Size = UDim2.new(1, 0, 0.2, 0)
        teamLabel.Position = UDim2.new(0, 0, 0.4, 0)
        teamLabel.TextScaled = true
        teamLabel.BackgroundTransparency = 1
        teamLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        teamLabel.Text = player.Team and player.Team.Name or "No Team"
        teamLabel.Parent = billboard
        teamLabel.Visible = showteamname

        local healthLabel = Instance.new("TextLabel")
        healthLabel.Size = UDim2.new(1, 0, 0.2, 0)
        healthLabel.Position = UDim2.new(0, 0, 0.6, 0)
        healthLabel.TextScaled = true
        healthLabel.BackgroundTransparency = 1
        healthLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        healthLabel.Text = "Health: " .. math.floor(humanoid.Health)
        healthLabel.Parent = billboard
        healthLabel.Visible = showhealth

        humanoid.HealthChanged:Connect(function(newHealth)
            healthLabel.Text = "Health: " .. math.floor(newHealth)
        end)

        if showteamcolor then
            local teamColor = player.TeamColor.Color
            nameLabel.TextColor3 = teamColor
            distanceLabel.TextColor3 = teamColor
            teamLabel.TextColor3 = teamColor
            healthLabel.TextColor3 = teamColor
        end

        RunService.RenderStepped:Connect(function()
            if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                local distance =
                    (player.Character.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).Magnitude
                distanceLabel.Text = string.format("Distance: %.2f", distance)
            end
        end)
    end
end

local function updateBillboards()
    for _, player in ipairs(Players:GetPlayers()) do
        if espname then
            createBillboard(player)
        else
            removeBillboard(player.Character)
        end
    end
end

